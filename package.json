{
  "name": "dubinq",
  "displayName": "Dubinq",
  "version": "0.0.1",
  "description": "Basic content",
  "author": "support@dubinq.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@google-cloud/translate": "^9.2.0",
    "google-translate-api-x": "^10.7.2",
    "googleapis": "^160.0.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "srt-parser-2": "^1.2.3",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "https://www.youtube.com/*",
      "https://translate.googleapis.com/*",
      "https://translation.googleapis.com/*"
    ],
    "permissions": [
      "storage",
      "activeTab",
      "tabs",
      "contextMenus",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "YouTube Translator"
    }
  }
}
